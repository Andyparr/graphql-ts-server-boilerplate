{
  "name": "graphql-ts-server-boilerplate",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/express": "^4.11.1",
    "@types/ioredis": "^3.2.7",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.0.4",
    "@types/node-fetch": "^1.6.9",
    "@types/sparkpost": "^2.1.3",
    "@types/uuid": "^3.4.3",
    "@types/yup": "^0.24.3",
    "gql2ts": "^1.8.0",
    "jest": "^22.4.3",
    "nodemon": "^1.17.4",
    "ts-jest": "^22.4.5",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "@gql2ts/from-schema": "^1.8.0",
    "apollo-server": "beta",
    "bcryptjs": "^2.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-import": "^0.5.2",
    "graphql-request": "^1.6.0",
    "graphql-tools": "^3.0.0",
    "graphql-yoga": "^1.13.1",
    "ioredis": "^3.2.2",
    "node-fetch": "^2.1.2",
    "pg": "^7.4.3",
    "reflect-metadata": "^0.1.12",
    "sparkpost": "^2.1.2",
    "typeorm": "^0.2.5",
    "uuid": "^3.2.1",
    "yup": "^0.24.1"
  },
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node -r dotenv/config src/index.ts",
    "test": "NODE_ENV=test jest --coverage --watch",
    "gen-schema-types": "ts-node -r dotenv/config src/scripts/createTypes.ts"
  },
  "jest": {
    "globalSetup": "./src/testSetup/callSetup.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}